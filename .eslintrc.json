{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style":"off",
        "valid-jsdoc": ["error", {"requireParamType": false,
            "requireReturnType": false}],
        "require-jsdoc":"warn"
    }
}
